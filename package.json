{
  "name": "hugo-blox-blog-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.30.3",
  "description": "Blog starter template for Hugo Blox with Tailwind CSS v4",
  "scripts": {
    "clean": "hugo mod clean && rm -rf _vendor node_modules resources public .hugo_build.lock",
    "new": "hugo new content",
    "dev": "hugo server -D -E -F -M -N -O -b http://localhost --bind 0.0.0.0 --disableFastRender --logLevel debug",
    "build": "hugo --gc --minify --logLevel debug && pnpm run pagefind",
    "pagefind": "pagefind --site public",
    "update": "pnpm update && hugo mod get -u && hugo mod tidy"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "pagefind": "^1.4.0",
    "preact": "^10.28.4",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "netlify-plugin-hugo-cache-resources": "^0.2.1"
  }
}
